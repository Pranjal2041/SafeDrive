{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true,
        "jquery": true
    },
    "extends": ["plugin:prettier/recommended", "airbnb-base", "prettier"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 11
    },
    "plugins": ["prettier"],
    "rules": {
        "no-console": "off",
        "camelcase": "off",
        "import/prefer-default-export": "off",
        "consistent-return": "off",
        "no-underscore-dangle": "off"
    }
}
